{
  "name": "frontend-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "reinstall-modules": "run-s clean:modules && yarn install",
    "clean:modules": "rimraf node_modules yarn.lock",
    "clean:build": "rimraf .next out build",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "control": "run-s lint format type-check test && yarn audit --production && echo \"\nControl finished!\n\"",
    "lint": "eslint --fix --ignore-path .eslintignore \"src/**/*.+(ts|tsx|js|jsx)\" && stylelint --fix --aei --config .stylelintrc \"src/**/*.+(css|scss|sass)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|js|jsx)\"",
    "type-check": "tsc",
    "dev": "next dev",
    "build:control": "run-s clean:build control && next build",
    "build": "run-s clean:build && next build",
    "export": "run-s build && next export",
    "start": "next start",
    "complete:start": "npm i && run-s build start"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "final-form": "4.20.7",
    "final-form-arrays": "3.0.2",
    "i18next": "^21.8.11",
    "immer": "^9.0.15",
    "lodash-es": "^4.17.21",
    "next": "^12.2.0",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.3",
    "react-i18next": "^11.17.4",
    "react-redux": "^8.0.2",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@next/env": "^12.2.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "4.0.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
